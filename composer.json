{
    "name": "gemorroj/stop-spam",
    "description": "Wrapper for API www.stopforumspam.com (api.stopforumspam.org)",
    "keywords": ["stopforumspam", "antispam", "blacklist", "spam-filtering", "spam-protection", "spam-detection"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "symfony/http-client": "^5.4||^6.0||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.46"
    },
    "autoload": {
        "psr-4": {
            "StopSpam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StopSpam\\Tests\\": "tests/"
        }
    }
}
