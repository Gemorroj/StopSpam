{
    "name": "gemorroj/stop-spam",
    "description": "Wrapper for API www.stopforumspam.com (api.stopforumspam.org)",
    "keywords": ["stopforumspam", "antispam", "blacklist", "spam-filtering", "spam-protection", "spam-detection"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/http-client": "^4.4.11|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "StopSpam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StopSpam\\Tests\\": "tests/"
        }
    }
}
